<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Test"
        tools:targetApi="31">
        <activity
            android:name=".ViewPage.StartViewPager"
            android:exported="false" />
        <activity
            android:name=".ThirdOfThird"
            android:exported="false"
            android:label="@string/title_activity_third_of_third"
            android:theme="@style/Theme.Test.NoActionBar" />
        <activity
            android:name=".SecondOfThree"
            android:exported="false"
            android:label="@string/title_activity_second_of_three"
            android:theme="@style/Theme.Test.NoActionBar" />
        <activity
            android:name=".OneOfThree"
            android:exported="false"
            android:label="@string/title_activity_one_of_three"
            android:theme="@style/Theme.Test.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".SplashScreen.mysplash"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Test.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>